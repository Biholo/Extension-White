{
    "name": "Whiteakitout Live Extension",
    "description": "Je te notifie quand je suis en live !",
    "version": "1.2",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
      },
    "action": {
        "default_popup": "main.html",
        "default_icon": {
            "16": "/css/images/get_started16.png",
            "32": "/css/images/get_started32.png",
            "48": "/css/images/get_started48.png",
            "128": "/css/images/get_started128.png"
          },
          "icons": {
            "16": "/css/images/get_started16.png",
            "32": "/css/images/get_started32.png",
            "48": "/css/images/get_started48.png",
            "128": "/css/images/get_started128.png"
          }
      },
      "permissions":[
          "alarms",
          "notifications"
      ]

      
  }